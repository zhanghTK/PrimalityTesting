<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>certainly</servlet-name>
        <servlet-class>tk.zhangh.action.CertainlyAction</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>certainly</servlet-name>
        <url-pattern>/certainly</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>MillerRabin</servlet-name>
        <servlet-class>tk.zhangh.action.MillerRabinAction</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MillerRabin</servlet-name>
        <url-pattern>/miller-rabin</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>SolovayStrassen</servlet-name>
        <servlet-class>tk.zhangh.action.SolovayStrassenAction</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SolovayStrassen</servlet-name>
        <url-pattern>/solovayStrassen</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>tk.zhangh.action.LoginAction</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>